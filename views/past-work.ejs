<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Past Work</title>
  </head>
  <%- include('./partials/nav') %>
  <body>
    <h1>Past Work</h1>
    <h2>Projects</h2>
    <a href="/past-work/divide and Conquer">
      <h3><p>Divide and Conquer</p></h3></a
    >
    <ul>
      <% pastWork.forEach(item=> { %> <% if (item.project === "Divide and Conquer") { %>
      <li>
        <p>Location: <%= item.location %></p>
        <p>Duration: <%= item.duration %></p>
        <p>Description: <%= item.description %></p>
      </li>
      <% }}); %>
    </ul>

    <a href="/past-work/mix and Sip">
